{
    "model": {
        "type": "FNN_2layer",
        "args": {
            "num_neurons_layer1": 50,
            "num_neurons_layer2": 50
        }
    },
    "optimizer": {
        "type": "SGD",
        "args": {
            "lr": 0.01,
            "momentum": 0.0,
            "weight_decay": 0.0
        }
    },
    "dataset": {
        "type": "iris",
        "args": {
            "random": false
        }
    },
    "init_model_at": "iris/random_label_training",
    "large_norm": false,
    "train_fraction": 0.8,
    "test_fraction": 0.2,
    "early_stopping": false,
    "trainer": {
        "num_epochs": 1000,
        "how_often_save_model": 0,
        "how_often_compute_grads": 0,
        "how_often_print": 100,
        "batch_size": "full"
    }
}