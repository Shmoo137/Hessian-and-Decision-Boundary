{
    "model": {
        "type": "FNN_2layer",
        "args": {
            "num_neurons_layer1": 100,
            "num_neurons_layer2": 100
        }
    },
    "optimizer": {
        "type": "SGD",
        "args": {
            "lr": 0.02,
            "momentum": 0.0,
            "weight_decay": 0.0
        }
    },
    "dataset": {
        "type": "gauss_mixtures",
        "args": {
            "random": true
        }
    },
    "init_model_at": "",
    "train_fraction": 0.5,
    "test_fraction": 0.2,
    "trainer": {
        "num_epochs": 40000,
        "how_often_save_model": 200,
        "how_often_compute_grads": 0,
        "how_often_print": 5000,
        "batch_size": 64
    }
}